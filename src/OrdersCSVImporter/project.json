{
  "version": "1.0.0-*",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": { "includeFiles": [ "nlog.config", "appsettings.json" ] }
  },

  "dependencies": {
    "TinyCsvParser": "1.5.0",
    "Microsoft.Extensions.Logging": "1.0.1",
    "Microsoft.Extensions.Logging.Console": "1.0.1",
    "Microsoft.Extensions.Logging.Debug": "1.0.1",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.1",
    "Microsoft.Extensions.DependencyInjection": "1.0.1",
    "Microsoft.Extensions.Configuration": "1.0.1",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.1",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.0.1",
    "Microsoft.Extensions.Configuration.Json": "1.0.1",
    "CommandLineParser": "2.1.1-beta",
    "NLog.Extensions.Logging": "1.0.0-*",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.3"
    },
    "Newtonsoft.Json": "9.0.1",
    "System.Runtime": "4.3.0",
    "runtime.any.System.Runtime": "4.1.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },
  "publishOptions": {
    "include": [
      "nlog.config",
      "appsettings.json"
    ]
  }
}
